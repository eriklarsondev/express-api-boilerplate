{
  "name": "express-api-boilerplate",
  "version": "0.1.0",
  "author": "Erik Larson",
  "description": "A fast, minimal Express.js boilerplate to kickstart your next custom API projectâ€”built with best practices in mind",
  "keywords": [],
  "license": "ISC",
  "main": "dist-server/app.js",
  "scripts": {
    "babel": "babel src --out-dir dist-server",
    "build": "npm run babel",
    "restart": "rimraf dist-server && npm run babel && node dist-server/app.js",
    "dev": "nodemon --exec npm run restart",
    "start": "npm run build && node dist-server/app.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
